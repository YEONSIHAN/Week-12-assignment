#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
// 배열 뒤집기
int main()
{
	vector<int> v = { 4, 1, 7, 3, 9 };
	reverse(v.begin(), v.end());

	for (int num : v)
	{
		cout << num << " ";
	}

#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
using namespace std;
// 문자열 정렬
int main()
{
	vector<string> v = { "c","b","a","d","e"};
	transform (v.begin(), v.end(), v.begin(),[](string s) {
			return s;
		});

	for (string num : v)
	{
		cout << num << " ";
	}
	
}

#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
using namespace std;
// 중복 제거
int main()
{
    vector<string> v = { "c","b","a","d","e", "c" };

    sort(v.begin(), v.end()); 

    auto last = unique(v.begin(), v.end());
    v.erase(last, v.end());

    for (string num : v)
    {
        cout << num << " ";
    }
    }

#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
// 원하는 값을 지우지만 실제로 메모리에서 제거하지는 않는다.
int main() {
    vector<int> v = { 1, 2, 3, 2, 4, 2, 5 };

    v.erase(remove(v.begin(), v.end(), 2), v.end());

    for (int x : v)
        cout << x << " ";
}


#include <iostream>
#include <vector>
using namespace std;
//하나의 요소를 지움
int main() {
    vector<int> v = { 10, 20, 30, 40 };

    v.erase(v.begin() + 1);

    for (int x : v) cout << x << " ";
}

    
